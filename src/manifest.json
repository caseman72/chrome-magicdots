{
	"permissions": [
		"notifications",
		"tabs",
		"*://*/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"version": "1.1.9",
	"name": "Magic Dots",
	"icons": {
		"128": "icon-128.png",
		"48": "icon-48.png",
		"16": "icon-16.png"
	},
	"description": "Run your magic dots here.",
	"update_url": "https://raw.github.com/caseman72/chrome-magicdots/master/update_manifest.xml",
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Magic Dots",
		"default_icon": "icon-19.png"
	},
	"manifest_version": 2
}
